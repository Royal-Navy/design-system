{
  "name": "@royalnavy/vue-component-library",
  "description": "A collection of react components and helpers to assist in building applications for the Royal Navy",
  "version": "0.1.1",
  "keywords": [
    "vue",
    "components"
  ],
  "author": "NELSON",
  "contributors": [
    "98dfsj <98dfsj@royalnavy.io> (royalnavy.io)",
    "hxltrhuxze <hxltrhuxze@royalnavy.io> (royalnavy.io)",
    "antictor <antictor@royalnavy.io> (royalnavy.io)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Royal-Navy/standards-toolkit"
  },
  "homepage": "https://github.com/Royal-Navy/standards-toolkit/tree/develop/packages/vue-component-library#readme",
  "license": "MIT",
  "main": "./dist/bundle.common.js",
  "files": [
    "dist"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vue-cli-service build --target lib --name bundle ./src/index.ts",
    "build-storybook": "build-storybook",
    "build-storybook-prod": "build-storybook -c .storybook -o .static_storybook",
    "lint": "eslint src",
    "lint-staged": "lint-staged",
    "prepublishOnly": "NODE_ENV=production npm run build",
    "storybook": "start-storybook -s ./static -p 3001",
    "svg": "vsvg -s ./static/assets/icons -t ./src/icons",
    "test": "jest --silent --no-cache",
    "test:ci": "JEST_JUNIT_OUTPUT=../../test-results/vue-components-unit.xml jest --ci --reporters=default --reporters=jest-junit",
    "test:watch": "jest --silent --watch"
  },
  "dependencies": {
    "uuid": "^3.3.2",
    "vue-svgicon": "^3.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@storybook/addon-actions": "^4.1.13",
    "@storybook/addon-links": "^4.1.13",
    "@storybook/addons": "^4.1.13",
    "@storybook/vue": "^4.1.13",
    "@types/jest": "^24.0.11",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__vue": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "@vue/cli-plugin-babel": "^3.5.3",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-plugin-typescript": "^3.5.3",
    "@vue/cli-plugin-unit-jest": "^3.5.3",
    "@vue/cli-service": "^3.5.3",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-vue": "^2.0.2",
    "css-loader": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-junit": "^1.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "jest": "^24.6.0",
    "jest-environment-jsdom-global": "^1.2.0",
    "jest-haste-map": "^24.6.0",
    "jest-junit": "^6.3.0",
    "jest-localstorage-mock": "^2.4.0",
    "jest-resolve": "^24.6.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^8.1.4",
    "node-sass": "^4.11.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "ts-jest": "^24.0.0",
    "typescript": "3.3.4000",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "^8.1.0",
    "vue-server-renderer": "^2.6.10",
    "vue-svg-inline-loader": "^1.2.15",
    "vue-template-compiler": "^2.6.10",
    "vue-template-loader": "^1.0.0",
    "webpack": "^4.0.0",
    "webpack-stream": "^5.2.1"
  },
  "peerDependencies": {
    "@royalnavy/css-framework": "0.1.1",
    "vue": "^2.6.10"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "vue"
    ],
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs",
          "transform-vue-jsx"
        ]
      }
    }
  },
  "lint-staged": {
    "*.@(js|vue|ts)": "eslint"
  },
  "jest": {
    "verbose": false,
    "moduleFileExtensions": [
      "js",
      "ts",
      "vue",
      "json"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/../../node_modules/babel-jest",
      "^.+\\.ts$": "<rootDir>/../../node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/../../node_modules/vue-jest"
    },
    "transformIgnorePatterns": [],
    "snapshotSerializers": [
      "<rootDir>/../../node_modules/jest-serializer-vue"
    ],
    "testEnvironment": "jest-environment-jsdom-global",
    "testMatch": [
      "**/?(*.)(spec|test).(js|ts)"
    ]
  }
}
