    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "puppeteer": "^5.2.1",
    "react": "^16.12.0",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.12.19",
    "sass": "^1.25.0",
    "sass-loader": "^10.0.2",
