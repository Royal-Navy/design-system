{
  "name": "@royalnavy/react-component-library",
  "description": "A collection of react components and helpers to assist in building applications for the Royal Navy",
  "version": "2.27.0",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "lib",
    "starter",
    "boilerplate"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "author": "Royal Navy",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Royal-Navy/design-system.git",
    "directory": "packages/react-component-library"
  },
  "scripts": {
    "analyze": "webpack -p --config=webpack/analyze.js",
    "build": "run-s build:es build:cjs types",
    "build:cjs": "NODE_ENV=cjs webpack -p --config=webpack/prod.js",
    "build:es": "NODE_ENV=es babel src --out-dir \"dist/es\" --extensions \".ts,.tsx\"",
    "build:dev": "RNDS_LOG_LEVEL=debug run-s build:ts:dev types",
    "build:ts:dev": "webpack -p --config=webpack/dev.js",
    "chromatic": "chromatic --build-script-name storybook:static --exit-zero-on-changes",
    "dev": "concurrently \"webpack --watch --config=webpack/dev.js\" \"yarn types --watch\"",
    "lint-staged": "lint-staged",
    "lint:ci": "eslint -f ../../node_modules/eslint-junit/index.js src/* --ext .js --ext .jsx --ext .ts --ext .tsx",
    "lint": "eslint src/* --ext .js --ext .jsx --ext .ts --ext .tsx",
    "prepare": "yarn build",
    "storybook:static": "build-storybook -c .storybook -o .static_storybook",
    "storybook": "RNDS_LOG_LEVEL=debug start-storybook -p 6006",
    "test": "NODE_ENV=test jest --testPathIgnorePatterns=./src/a11y/index.test.tsx",
    "test:a11y": "NODE_ENV=test yarn storybook:static && jest ./src/a11y/index.test.tsx",
    "test:watch": "NODE_ENV=test jest --watch",
    "types": "tsc"
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": "eslint"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@royalnavy/eslint-config-react": "^2.27.0",
    "@storybook/addon-a11y": "^6.1.9",
    "@storybook/addon-actions": "^6.1.9",
    "@storybook/addon-knobs": "^6.1.9",
    "@storybook/addon-links": "^6.1.9",
    "@storybook/addon-storyshots": "^6.1.9",
