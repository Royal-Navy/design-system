{
  "name": "@defencedigital/react-component-library",
  "description": "A collection of react components and helpers to assist in building applications for the Defence Digital",
  "version": "2.78.1",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "lib",
    "starter",
    "boilerplate"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "author": "Defence Digital",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/defencedigital/mod-uk-design-system.git",
    "directory": "packages/react-component-library"
  },
  "scripts": {
    "analyze": "webpack --mode production --config=webpack/analyze.js",
    "build": "run-s build:es build:cjs types",
    "build:cjs": "BABEL_ENV=cjs webpack --mode production --config=webpack/prod.js",
    "build:es": "BABEL_ENV=es babel src --out-dir \"dist/es\" --extensions \".ts,.tsx\"",
    "build:dev": "RNDS_LOG_LEVEL=debug run-s build:ts:dev types",
    "build:ts:dev": "webpack --mode production --config=webpack/dev.js",
    "chromatic": "chromatic --build-script-name storybook:static --exit-zero-on-changes",
    "dev": "concurrently \"webpack --watch --config=webpack/dev.js\" \"yarn types --watch\"",
    "lint-staged": "lint-staged",
    "lint": "eslint src cypress --ext .js --ext .jsx --ext .ts --ext .tsx",
    "prepare": "yarn build",
    "storybook": "BABEL_ENV=es RNDS_LOG_LEVEL=debug start-storybook -p 6006",
    "storybook:static": "BABEL_ENV=es build-storybook -c .storybook -o .static_storybook",
    "storybook:test": "BABEL_ENV=test RNDS_LOG_LEVEL=debug start-storybook -p 6006",
    "test": "BABEL_ENV=test jest --testPathIgnorePatterns=./src/a11y/index.test.tsx",
    "test:a11y": "BABEL_ENV=test yarn storybook:static && jest ./src/a11y/index.test.tsx",
    "test:watch": "BABEL_ENV=test jest --watch",
    "test:e2e": "cypress run",
    "types": "tsc"
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": "eslint"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "rcl-sonar-report.xml",
    "indent": 4
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@defencedigital/eslint-config-react": "^2.78.1",
    "@storybook/addon-a11y": "^6.4.1",
    "@storybook/addon-actions": "^6.4.1",
    "@storybook/addon-docs": "^6.4.1",
    "@storybook/addon-storyshots": "^6.4.1",
    "@storybook/addon-storyshots-puppeteer": "^6.4.1",
    "@storybook/addons": "^6.4.1",
    "@storybook/builder-webpack5": "^6.4.1",
    "@storybook/manager-webpack5": "^6.4.1",
    "@storybook/react": "^6.4.1",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/user-event": "^13.1.1",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^27.0.2",
    "@types/jsdom": "^16.2.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^16.3.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-select": "^3.0.27",
    "@types/react-transition-group": "^4.2.3",
    "@types/styled-components": "^5.1.3",
    "@types/styled-theming": "^2.2.5",
