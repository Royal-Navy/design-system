{
  "name": "@royalnavy/react-component-library",
  "description": "A collection of react components and helpers to assist in building applications for the Royal Navy",
  "version": "2.2.0",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "lib",
    "starter",
    "boilerplate"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "author": "NELSON",
  "license": "Apache-2.0",
  "scripts": {
    "analyze": "webpack -p --config=webpack/analyze.js",
    "build": "run-s build:es build:cjs types",
    "build:cjs": "NODE_ENV=cjs webpack -p --config=webpack/prod.js",
    "build:es": "NODE_ENV=es babel src --out-dir \"dist/es\" --extensions \".ts,.tsx\"",
    "build:dev": "run-s build:ts:dev types",
    "build:ts:dev": "webpack -p --config=webpack/dev.js",
    "chromatic": "chromatic --build-script-name storybook:static --exit-zero-on-changes",
    "dev": "concurrently \"webpack --watch --config=webpack/dev.js\" \"yarn types --watch\"",
    "lint-staged": "lint-staged",
    "lint:ci": "eslint -f ../../node_modules/eslint-junit/index.js src/* --ext .js --ext .jsx --ext .ts --ext .tsx",
    "lint": "eslint src/* --ext .js --ext .jsx --ext .ts --ext .tsx",
    "prepare": "yarn build",
    "storybook:static": "build-storybook -c .storybook -o .static_storybook",
    "storybook": "start-storybook -p 6006",
    "test:watch": "NODE_ENV=test jest --watch",
    "test": "NODE_ENV=test jest",
    "types": "tsc --emitDeclarationOnly --declarationMap --declaration --noEmit false --isolatedMOdules false --allowJs false --outDir dist/types"
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": "eslint"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@storybook/addon-actions": "^5.3.9",
    "@storybook/addon-knobs": "^5.3.9",
    "@storybook/addon-links": "^5.3.9",
    "@storybook/addons": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "@svgr/webpack": "^5.1.0",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^25.1.0",
    "@types/jsdom": "^12.2.4",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-select": "^3.0.10",
    "@types/react-transition-group": "^4.2.3",
    "@types/storybook__react": "^4.0.2",
    "@types/uuid": "^3.4.6",
    "@types/yup": "^0.26.28",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-junit": "^1.0.1",
    "eslint-plugin-import": "~2.18.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "express": "^4.17.1",
    "file-loader": "^5.1.0",
    "formik": "^2.1.2",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.1.0",
    "jest-junit": "^9.0.0",
    "jsdom": "^16.0.1",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "storybook-chromatic": "^3.4.1",
    "style-loader": "^1.1.3",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "yup": "^0.27.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all",
    "ie 11",
    "chrome >= 38"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "@datepicker-react/hooks": "^2.3.1",
    "@royalnavy/css-framework": "^2.2.0",
    "@royalnavy/fonts": "^2.2.0",
    "@royalnavy/icon-library": "^2.2.0",
    "@types/react-responsive": "^8.0.2",
    "@types/react-toast-notifications": "^2.4.0",
    "classnames": "^2.2.6",
    "date-fns": "^2.9.0",
    "react-children-utilities": "^1.3.3",
    "react-compound-slider": "^2.5.0",
    "react-indiana-drag-scroll": "^1.5.5",
    "react-responsive": "^8.0.3",
    "react-select": "^3.0.8",
    "react-tether": "^2.0.7",
    "react-toast-notifications": "^2.4.0",
    "react-transition-group": "^4.3.0"
  }
}
