{
  "name": "@royalnavy/react-component-library",
  "description": "A collection of react components and helpers to assist in building applications for the Royal Navy",
  "version": "1.2.1",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "lib",
    "starter",
    "boilerplate"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "NELSON",
  "license": "Apache-2.0",
  "scripts": {
    "analyze": "webpack -p --config=webpack/analyze.js",
    "build": "run-s build:ts types",
    "build:dev": "run-s build:ts:dev types",
    "build:ts": "webpack -p --config=webpack/prod.js",
    "build:ts:dev": "webpack -p --config=webpack/dev.js",
    "dev": "concurrently \"webpack --watch --config=webpack/dev.js\" \"yarn types --watch\"",
    "lint": "eslint src/* --ext .js --ext .jsx --ext .ts --ext .tsx",
    "lint:ci": "eslint -f ../../node_modules/eslint-junit/index.js src/* --ext .js --ext .jsx --ext .ts --ext .tsx",
    "storybook": "start-storybook -p 6006",
    "storybook:static": "build-storybook -c .storybook -o .static_storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "types": "tsc --emitDeclarationOnly --declarationMap --declaration --noEmit false --isolatedMOdules false --allowJs false --outDir dist/",
    "prepare": "yarn build"
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": "eslint"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@svgr/webpack": "^4.3.2",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^12.7.4",
    "@types/react": "^16.9.2",
    "@types/react-addons-css-transition-group": "^15.0.5",
    "@types/react-dom": "^16.9.0",
    "@types/react-select": "^3.0.4",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-knobs": "^5.0.3",
    "@types/storybook__react": "^4.0.2",
    "@types/uuid": "^3.4.5",
    "@types/yup": "^0.26.23",
    "@typescript-eslint/eslint-plugin": "^2.1.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.2.0",
    "eslint-junit": "^1.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "formik": "^1.5.8",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "jsdom": "^15.1.1",
    "lint-staged": "^9.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.12",
    "sass": "^1.22.10",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-merge": "^4.2.2",
    "yup": "^0.27.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all",
    "ie 11",
    "chrome >= 38"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
    "@types/react-responsive": "^3.0.3",
    "@royalnavy/css-framework": "^1.2.1",
    "@royalnavy/icon-library": "^1.2.1",
    "classnames": "^2.2.6",
    "react-addons-css-transition-group": "^15.6.2",
    "react-children-utilities": "^1.3.1",
    "react-indiana-drag-scroll": "^1.3.0",
    "react-responsive": "^8.0.1",
    "react-select": "^3.0.4"
  }
}
