{
  "name": "@defencedigital/icon-library",
  "version": "3.5.0",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "author": "Defence Digital",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/defencedigital/mod-uk-design-system.git",
    "directory": "packages/icon-library"
  },
  "scripts": {
    "build": "run-s svgr-icons svgr-silhouettes exports build:es build:cjs types",
    "build:es": "BABEL_ENV=es babel src --out-dir \"dist/es\" --extensions \".ts,.tsx\"",
    "build:cjs": "BABEL_ENV=cjs webpack --mode production --config=webpack/prod.js",
    "svgr-icons": "rm -rf src/icons; svgr -d src/icons -- src/assets/icons/**/; rm src/icons/index.ts",
    "svgr-silhouettes": "rm -rf src/silhouettes; svgr -d src/silhouettes -- src/assets/silhouettes/; rm src/silhouettes/index.ts",
    "exports": "./generate-exports.sh",
    "types": "tsc --emitDeclarationOnly --declarationMap --declaration --noEmit false --allowJs false --outDir dist/types",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "react-svg-unique-id": "^1.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@defencedigital/eslint-config-react": "^3.5.0",
    "@svgr/cli": "^6.1.2",
    "@types/react": "^17.0.38",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "source-map-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.3.0",
    "typescript": "^4.1.2",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.2.0"
  }
}
