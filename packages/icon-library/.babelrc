{
  "env": {
    "es": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false
          }
        ],
        "@babel/preset-react",
        "@babel/typescript"
      ]
    },
    "cjs": {
      "presets": [
        [
          "@babel/preset-env"
        ],
        "@babel/preset-react",
        "@babel/typescript"
      ]
    }
  },
  "plugins": [
    [
      "@svgr/babel-plugin-add-jsx-attribute",
      {
        "elements": ["svg"],
        "attributes": [
          {
            "name": "width",
            "value": "size",
            "spread": false,
            "literal": true
          },
          {
            "name": "height",
            "value": "size",
            "spread": false,
            "literal": true
          },
          {
            "name": "fill",
            "value": "CurrentColor",
            "spread": false,
            "literal": false,
            "position": "end"
          }
        ]
      }
    ]
  ]
}
