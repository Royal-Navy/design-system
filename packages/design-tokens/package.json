{
  "name": "@royalnavy/design-tokens",
  "version": "4.5.1",
  "main": "dist/cjs/index.js",
  "author": "Royal Navy",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/Royal-Navy/design-system.git",
    "directory": "packages/design-tokens"
  },
  "scripts": {
    "build": "run-s build:dict build:cjs types",
    "build:dict": "node ./build-dictionary.js",
    "build:cjs": "webpack --config=webpack/prod.js",
    "types": "tsc --emitDeclarationOnly --declarationMap --declaration --noEmit false --allowJs false --outDir dist/types",
    "prepare": "yarn build",
    "test": "jest",
    "lint": "eslint src --ext .js --ext .jsx --ext .ts --ext .tsx --ext .json --max-warnings 0 --resolve-plugins-relative-to ../eslint-config-react"
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": [
      "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --ext .json --max-warnings 0 --resolve-plugins-relative-to ../eslint-config-react",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@royalnavy/eslint-config-react": "^4.5.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "@types/jest": "^29.0.3",
    "@types/lodash": "^4.14.149",
    "@types/react": "^18.2.25",
    "@types/styled-components": "^5.1.34",
    "babel-loader": "^9.0.1",
    "babel-plugin-styled-components": "^2.0.2",
    "clean-webpack-plugin": "^4.0.0",
    "eslint": "^8.2.0",
    "jest": "^29.0.3",
    "jest-styled-components": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "source-map-loader": "^4.0.1",
    "style-dictionary": "^3.1.1",
    "styled-components": "^6.1.8",
    "typescript": "^5.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.2.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "styled-components": ">= 6"
  }
}
