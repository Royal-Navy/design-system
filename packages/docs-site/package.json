{
  "name": "royalnavy.io",
  "engines": {
    "node": ">=10.13.0 <13"
  },
  "description": "The documentation site for royalnavy.io",
  "version": "2.2.0",
  "author": "NELSON",
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn cache:clear && yarn svgr && gatsby build",
    "develop": "yarn svgr && gatsby develop",
    "develop:clean": "yarn cache:clear && yarn svgr && gatsby develop",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src",
    "lint:ci": "eslint -f ../../node_modules/eslint-junit/index.js src",
    "test": "jest",
    "cache:clear": "rm -rf ./.cache",
    "svgr": "yarn svgr:clear && yarn svgr:build",
    "svgr:clear": "find src -regex '.*/images/.*.js' -delete",
    "svgr:build": "svgr -d src src"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Royal-Navy/standards-toolkit"
  },
  "bugs": {
    "url": "https://github.com/Royal-Navy/standards-toolkit/issues"
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": "eslint"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.5",
    "@mdx-js/react": "^1.5.5",
    "@royalnavy/css-framework": "^2.2.0",
    "@royalnavy/icon-library": "^2.2.0",
    "@royalnavy/react-component-library": "^2.2.0",
    "change-case": "^3.1.0",
    "gatsby": "^2.19.5",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-catch-links": "^2.1.24",
    "gatsby-plugin-manifest": "^2.2.38",
    "gatsby-plugin-mdx": "^1.0.67",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sass-resources": "^2.0.0",
    "gatsby-plugin-sharp": "^2.4.0",
    "gatsby-plugin-styled-components": "^3.1.18",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-sharp": "^2.3.13",
    "lodash": "^4.17.15",
    "mixin-deep": "^2.0.1",
    "normalize-scss": "^7.0.1",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^4.4.1",
    "to-style": "^1.3.3",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@royalnavy/eslint-config-react": "^2.2.0",
    "@svgr/cli": "^5.1.0",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^9.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-preset-gatsby": "^0.2.27",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-junit": "^1.0.1",
    "eslint-plugin-import": "~2.18.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "formik": "^2.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jsdom": "^16.0.1",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "sass": "^1.25.0",
    "stylelint": "^13.0.0",
    "stylelint-junit-formatter": "^0.2.2"
  }
}
