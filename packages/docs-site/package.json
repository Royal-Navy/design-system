{
  "name": "royalnavy.io",
  "engines": {
    "node": ">=12.18.3 <15"
  },
  "description": "The documentation site for royalnavy.io",
  "version": "2.54.1",
  "author": "Royal Navy",
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn cache:clear && yarn svgr && gatsby build",
    "develop": "yarn svgr && gatsby develop",
    "develop:clean": "yarn cache:clear && yarn svgr && gatsby develop",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src",
    "lint:ci": "eslint -f ../../node_modules/eslint-junit/index.js src",
    "test": "jest",
    "cache:clear": "rm -rf ./.cache",
    "svgr": "yarn svgr:clear && yarn svgr:build",
    "svgr:clear": "find src -regex '.*/images/.*.js' -delete",
    "svgr:build": "svgr -d src src",
    "versions:update": "node ./scripts/versions/update.js",
    "versions:commit": "./scripts/versions/commit.sh",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Royal-Navy/design-system.git",
    "directory": "packages/docs-site"
  },
  "bugs": {
    "url": "https://github.com/Royal-Navy/design-system/issues"
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": "eslint"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.5",
    "@mdx-js/react": "^1.5.5",
    "@royalnavy/css-framework": "^2.54.1",
    "@royalnavy/fonts": "^2.54.1",
    "@royalnavy/icon-library": "^2.54.1",
    "@royalnavy/react-component-library": "^2.54.1",
    "change-case": "^4.1.1",
    "firacode": "^5.2.0",
    "gatsby": "3.8.0",
    "gatsby-image": "^3.2.0",
    "gatsby-plugin-catch-links": "^3.5.0",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-mdx": "^2.1.0",
    "gatsby-plugin-offline": "^4.5.0",
    "gatsby-plugin-react-helmet": "^4.5.0",
    "gatsby-plugin-sass": "^4.5.0",
    "gatsby-plugin-sass-resources": "^2.0.0",
    "gatsby-plugin-styled-components": "^4.5.0",
    "gatsby-source-filesystem": "^3.5.0",
    "lodash": "^4.17.19",
    "mixin-deep": "^2.0.1",
    "normalize-scss": "^7.0.1",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "snyk": "^1.334.0",
    "to-style": "^1.3.3",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@royalnavy/eslint-config-react": "^2.54.1",
    "@svgr/cli": "^5.1.0",
    "@testing-library/jest-dom": "^5.0.2",
    "@testing-library/react": "^11.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-preset-gatsby": "^1.0.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-junit": "^1.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "formik": "^2.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jsdom": "^16.0.1",
    "lint-staged": "^10.2.13",
    "prettier": "^2.0.5",
    "react-test-renderer": "^17.0.1",
    "sass": "^1.25.0",
    "stylelint": "^13.0.0",
    "stylelint-junit-formatter": "^0.2.2"
  },
  "snyk": true
}
