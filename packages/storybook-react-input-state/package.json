{
  "name": "@royalnavy/storybook-react-input-state",
  "description": "State wrapper for form components in react storybook stories",
  "version": "1.5.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "keywords": [
    "storybook",
    "addon",
    "react",
    "state"
  ],
  "author": "NELSON",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Royal-Navy/nelson-frontend-component-library.git"
  },
  "homepage": "https://github.com/Royal-Navy/nelson-frontend-component-library",
  "bugs": {
    "url": "https://github.com/Royal-Navy/nelson-frontend-component-library/issues"
  },
  "scripts": {
    "build": "webpack",
    "build-storybook": "build-storybook",
    "lint": "eslint src",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "test": "jest --silent --no-cache"
  },
  "peerDependencies": {
    "react": "^16.9.0 | ^16.10.0",
    "react-dom": "^16.9.0 | ^16.10.0"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@storybook/react": "^5.2.3",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "jest-junit": "^8.0.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "source-map-loader": "^0.2.4",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "jest": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "parser": "babel-eslint"
  },
  "prettier": {
    "bracketSpacing": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "lint-staged": {
    "*.@(js)": "eslint"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ]
  }
}
